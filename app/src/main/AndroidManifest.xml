<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alunos.prodapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/prod"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/prod"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".TelaDeCadastro" />
        <activity android:name=".TelaDeConfiguracaoDeSilo" />
        <activity android:name=".TelaDeLogin" />
        <activity android:name=".ListaUsuarios" />
        <activity android:name=".ListaSilos" />
        <activity
            android:name=".MenuLateralActivity"
            android:label="@string/title_activity_menu_lateral"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".TelaCadastroSilos" />
        <activity android:name=".TelaDeGerenciamentoDeSilo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>